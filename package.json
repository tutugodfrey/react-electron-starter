{
  "name": "react-electron-starter",
  "version": "1.0.0",
  "description": "A simple boiler-plate to creating electron apps with react",
  "keywords": [
    "react",
    "electron",
    "node",
    "scss",
    "webpack"
  ],
  "main": "build/main.js",
  "build": {
    "productName": "react-electron",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "files": [
      "**/*",
      "build/**/*",
      "node_modules/**/*"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --mode production",
    "electron-build": "npm run build && npm run build-app && electron-builder",
    "build-app": "babel main.js --out-file build/main.js",
    "start": "npm run build-app && electron build/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tutugodfrey/react-electron-starter.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tutugodfrey/react-electron-starter/issues"
  },
  "homepage": "https://github.com/tutugodfrey/react-electron-starter#readme",
  "dependencies": {
    "esm": "^3.2.25",
    "path": "^0.12.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "electron": "^5.0.3",
    "electron-builder": "^20.43.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4"
  }
}
